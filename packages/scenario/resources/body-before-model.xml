<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml">
    <h:body>
        <input ref="/data/text"/>
    </h:body>
    <h:head>
        <h:title>Body Before Model</h:title>
        <model>
            <instance>
                <data id="body-before-model">
                    <text/>
                </data>
            </instance>
            <bind id="text-data-binding" nodeset="/data/text" type="string"/>
        </model>
    </h:head>
</h:html>